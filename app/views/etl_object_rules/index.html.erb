<div class="row">
  <div class="small-12 columns">
    <div class="pan">
      <p><%= @pre %></p>
    </div>
  </div>
  <div class="small-12 columns dark">
    <h1>Object Rules <%= link_to 'New Etl object rule', new_etl_object_rule_path, class: 'right button inline radius' %></h1>

    <table>
      <thead>
        <tr>
          <th>Extract Object</th>
          <th>Load Object</th>
          <th>Active</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @etl_object_rules.each do |etl_object_rule| %>
          <tr>
            <td><%= etl_object_rule.extract_object %></td>
            <td><%= etl_object_rule.load_object %></td>
            <td><%= etl_object_rule.active %></td>
            <td><%= link_to 'Show', etl_object_rule %></td>
            <td><%= link_to 'Edit', edit_etl_object_rule_path(etl_object_rule) %></td>
            <td><%= link_to 'Destroy', etl_object_rule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>
  </div>
</div>