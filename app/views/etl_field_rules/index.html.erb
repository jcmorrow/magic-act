<div class="row">
  <div class="small-12 columns dark">
    <h1>Field Rules <%= link_to 'New Etl field rule', new_etl_object_rule_etl_field_rule_path, class: 'right button inline radius' %>
</h1>
    <table>
      <thead>
        <tr>
          <th>Extract field</th>
          <th>Transformation</th>
          <th>Load field</th>
          <th>Active</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @etl_field_rules.each do |etl_field_rule| %>
          <tr>
            <td><%= etl_field_rule.extract_field %></td>
            <td><%= etl_field_rule.transformation %></td>
            <td><%= etl_field_rule.load_field %></td>
            <td><%= etl_field_rule.active %></td>
            <td><%= link_to 'Show', etl_object_rule_etl_field_rule_path(:etl_object_rule_id => etl_field_rule.etl_object_rule_id, :id => etl_field_rule.id) %></td>
            <td><%= link_to 'Edit', edit_etl_object_rule_etl_field_rule_path(:etl_object_rule_id => etl_field_rule.etl_object_rule_id, :id => etl_field_rule.id) %></td>
            <td><%= link_to 'Destroy', etl_object_rule_etl_field_rule_path(:etl_object_rule_id => etl_field_rule.etl_object_rule_id, :id => etl_field_rule.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>
  </div>
</div>